{
    "name": "gsmanager/laravel-authentication-log",
    "description": "Laravel Authentication Log provides authentication logger and notification for Laravel.",
    "keywords": [
        "laravel",
        "authentication",
        "log",
        "notification",
        "gsmanager"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Tyukhtin",
            "email": "info@gsmanager.ru"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/auth": "^10.0|^11.0|^12.0",
        "illuminate/bus": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/notifications": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "../vendor/phpunit/phpunit": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "GSManager\\AuthenticationLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GSManager\\AuthenticationLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "GSManager\\AuthenticationLog\\AuthenticationLogServiceProvider"
            ]
        }
    },
    "suggest": {
        "../vendor/guzzlehttp/guzzle": "Required to use the Slack transport (~6.0)",
        "nexmo/client": "Required to use the Nexmo transport (~1.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
